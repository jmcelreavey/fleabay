{
  "name": "@fleabay/db",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "dev": "pnpm db:start",
    "db": "pnpm with-env drizzle-kit --",
    "db:start": "docker compose up -d && tsx ./docker-await-mysql.ts",
    "db:push": "pnpm db:start && pnpm with-env drizzle-kit push:mysql -- --verbose",
    "db:studio": "pnpm with-env drizzle-kit studio --verbose",
    "db:generate": "pnpm with-env drizzle-kit generate:mysql",
    "clean": "rm -rf .turbo node_modules",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "@auth/core": "^0.18.6",
    "@planetscale/database": "^1.13.0",
    "drizzle-orm": "0.29.1",
    "mysql2": "^3.6.5",
    "nanoid": "^5.0.4",
    "uploadthing": "^6.1.0"
  },
  "devDependencies": {
    "@fleabay/eslint-config": "workspace:*",
    "@fleabay/prettier-config": "workspace:*",
    "@fleabay/tsconfig": "workspace:*",
    "@types/node": "^20.10.5",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "0.20.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@fleabay/eslint-config/base"
    ]
  },
  "prettier": "@fleabay/prettier-config"
}
